define(function () {
    	'use strict';
   	 function ctrl($scope,CarWashRequest,$ionicTabsDelegate,getGoodList,getTokenR,$timeout,getId,customePopu,confirmPopu,$state) {
	    	
	    	
	    	/*为跳转订单详情页 获取id*/
	    	$scope.getId=function(id){
	    		getId.addIdName(id);
	    	};
    		$scope.on_select=function(status){
			 /*发送参数*/
             getToken(function(token){
                var sendParas={};
                $scope.token=token;
                 sendParas.token=$scope.token;
                 if(status==="5"){
                    sendParas.evaluation="0"
                 }
                 sendParas.status=status;
                 console.log(sendParas)
                 var index=$ionicTabsDelegate.selectedIndex();
                 CarWashRequest.runHttp("get","/my/orderList",sendParas).success(function(response){
                    console.log(response);
                    $scope.allData=response.result;
                 }).error(function(response) {
                     /* Act on the event */
                     /*错误处理*/
                     console.log(response)
                 });
             })
    			 

		 };
    		/*过滤状态*/
    		$scope.whichStaus=function(status){
    			if(status==="0"){
    				return "待付款";/*有*/
    			}else if(status==="1"){
    				return "买家已付款";/*有*/ 
    			}else if(status==="2"){
    				return "备货中";/*无*/
    			}else if(status==="3"){ /*视图有*/
    				return "待发货";/*无*/
    			}else if(status==="4"){/*视图有*/
    				return "待收货";/*有*/
    			}else if(status==="5"){/*视图有*/
    				return "交易成功";/*有 交易成功 未评价 */
    			}else if(status==="6"){
    				return "退货中"/*无*/
    			} else if(status==="7"){
    				return "交易关闭"/*有*/
    			}
    		};
    		$scope.goWhichPage=function(status){
    			if(status==="1"){
    				return "order-unpay";
    			}else if(status==="4"){
    				return "order-successed"
    			}else if(status==="5"){
    				return "order-payed"
    			}else if(status==="7"){
    				return "order-closed"
    			}
    		};
    		/*点击跳转评价*/
    		$scope.goComment=function(obj,$event){

    			$event.stopPropagation();
    			getGoodList.addGoods(obj);
                $state.go("comment")
    		};    
    		/*发送取消订单*/
    		$scope.cancleOrder=function(id,order){
    			var sendParas={};
    			sendParas.id=id;
    			sendParas.token=$scope.token;
    			sendParas.type="2";
			confirmPopu("确定取消").then(function(res){
				if(res){
					CarWashRequest.runHttp("post","/my/editOrder",sendParas).success(function(response){
    						console.log(response);
    						/*更改订单的状态*/
    						if(response.code===0){
    							order.order_status="7";
    							/*customePopu("取消成功");*/
                                mallPrompt("取消成功")

    						}
    					}).error(function(response) {
    						/* Act on the event *//*错误处理*/
    						console.log(response)
    					});
				}else{

				}
			})
    			
    		};
    		/*发送订单请求*/
    		$scope.deleteOrder=function(id,index){
    			var sendParas={};
    			sendParas.id=id;
    			sendParas.type="1";
    			sendParas.token=$scope.token;
    			console.log(sendParas)
    			/*确认对话框*/
    			confirmPopu("确定删除").then(function(res){
    				if(res){
    					/*确定删除*/
    					CarWashRequest.runHttp("post","/my/editOrder",sendParas).success(function(response){
    						console.log(response);
    						if(response.code===0){
    							$scope.allData.splice(index,1);
    							/*提示对话框*/
    							/*customePopu("删除成功");*/
                                mallPrompt("删除成功")
    						}
    					}).error(function(response) {
    						/* Act on the event */
    						console.log(response)
    					});
    				}else{
    					/*不确定删除取消*/
    					console.log("不确定")
    				}
    			})
    		}
	}
    ctrl.$inject = ['$scope',"CarWashRequest","$ionicTabsDelegate","getGoodList","getTokenR","$timeout","getId","customePopu","confirmPopu","$state"];
    return ctrl;
});



