.text-line-2 {
    /*多行(2行)*/
    word-break:break-all;
    display:-webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
    overflow:hidden !important;

}
.text-line-1 {
    /*多行(1行)*/
    word-break:break-all;

    -webkit-line-clamp:1;
    -webkit-box-orient:vertical;
    overflow:hidden !important;
    white-space: nowrap !important;
    text-overflow: ellipsis !important;
}



